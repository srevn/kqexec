<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.kqexec.daemon</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/kqexec</string>
        <string>-c</string>
        <string>/usr/local/etc/kqexec.conf</string>
        <string>-d</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/var/log/kqexec.err</string>
    <key>StandardOutPath</key>
    <string>/var/log/kqexec.log</string>
</dict>
</plist>